<h1> Create a new bookmark </h1>

<%= simple_form_for(@bookmark) do |f| %>

  <div class="form-inputs">
    <%= f.input :comment %>
     <%= f.input :list_id %>
      <%= f.input :movie_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-primary' %>
  </div>
<% end %>
